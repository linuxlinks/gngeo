raze.asmu : raze.asm
	$(srcdir)/strip_ $< $@
raze.incu : raze.inc
	$(srcdir)/strip_ $< $@
raze.regu : raze.reg
	$(srcdir)/strip_ $< $@

raze2.asm: raze.asmu raze.incu raze.regu
	nasm -w+orphan-labels -I $(srcdir)/ -e $< -o $@

# This assembles the file, once it's been pre-processed
raze.o: raze2.asm
	nasm -w+orphan-labels -f elf -I $(srcdir)/ $< -o $@ -praze.regu

